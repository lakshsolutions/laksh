    <!--=========== BEGAIN CONTACT SECTION ================-->
    <section id="contact">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12">
            <!-- START CONTACT HEADING -->
            <div class="heading">
              <h2 class="wow fadeInLeftBig">Contact Us</h2>
              <p>There are many variations of passages of Lorem Ipsum available, but the majority have suffered alteration in some form, by injected humour, or randomised words which don't look even slightly believable. </p>
            </div>
          </div>
        </div>
        <div class="row">
          <!-- BEGAIN CONTACT CONTENT -->
          <div class="contact_content">
            <!-- BEGAIN CONTACT FORM -->
            <div class="col-lg-5 col-md-5 col-sm-5">
              <div class="contact_form">
                <!-- FOR CONTACT FORM MESSAGE -->
                <div id="form-messages"></div>
                  <%= form_tag(contact_us_path, method: 'post') do %>
                    <%= text_field_tag :name, '', required: true, class: 'form-control', placeholder: 'Name' %>
                    <%= email_field_tag :email, '', required: true, class: 'form-control', placeholder: 'Email' %>
                    <%= number_field_tag :phone_no, '', class: 'form-control', placeholder: 'Phone Number' %>
                    <%= text_field_tag :subject, '', required: true, class: 'form-control', placeholder: 'Subject' %>
                    <%= text_area_tag :message, '', required: true, cols: 30, rows: 10, class: 'form-control', placeholder: 'Your Message' %>
                    <div class="footer-submit-btn">
                      <%= button_tag 'Send', class: 'send-btn' %>
                    </div>
                  <% end %>
              </div>
            </div>
            <!-- BEGAIN CONTACT MAP -->
            <div class="col-lg-7 col-md-7 col-sm-7">
              <div class="contact_map">
              <!-- BEGAIN GOOGLE MAP -->
                <div>
                  <%= image_tag 'full-slide1.jpg', class: 'map_image' %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
